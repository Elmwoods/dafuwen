page {
  height: 100%;
  background-color: #fff;
}
.container{
  height: 100%;
  font-size: 28rpx;
}
.module_top{
  width: 100%;
  height: 336rpx;
}
.module_top image{
  width: 100%;
  height: 100%;
}
.help_nav{
  width: 100%;
  height: 158rpx;
  text-align: center;
  box-sizing: border-box;
  padding: 0rpx 20rpx;
  margin: -70rpx auto 0;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.help_nav .help_nav_li{
  display: inline-block;
  width: 35%;
  height:150rpx;
  line-height:150rpx;
  text-align:center;
  margin:0 6%;
}
.help_nav image{
  display:inline-block;
  vertical-align:middle;
  width:68rpx;
  height:66rpx;
  margin-right:15rpx;
}
.help_nav .help_nav_li text{
  display:inline-block;
  height:144rpx;
  line-height:144rpx;
  text-align:center;
}
.line{
  display: inline-block;
  width: 2rpx;
  height: 80rpx;
  background-color: #e3e3e3;
  vertical-align: middle;
}

.activited {
  border-bottom: 2rpx #3850e8 solid;
  color: #3850e8 ;
}
.txt{
  width: 100%;
  color: #666;
  font-size: 30rpx;
  margin: 30rpx 5%;
}
.txt_title{
  color: #333;
  margin-bottom: 20rpx;
}