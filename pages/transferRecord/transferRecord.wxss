page {
  height: 100%;
  background-color: #f1f1f1;
}
.container{
  height: 100%;
  font-size: 28rpx;
}
.navbar{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top: 1px solid #f1f1f1;
}
.navbar view{
  display: inline-block;
  width: 25%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.navbar view text{
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
}
.navbar view text.activited {
  border-bottom: 2rpx #3850e8 solid;
  color: #3850e8 ;
}
.transfer_detail{
  width: 100%;
  color: #333;
  box-sizing: border-box;
  padding: 0rpx 26rpx;
  
}
.transfer_one{
  width: 100%;
  font-size: 28rpx;
  margin-top: 30rpx;
  border-radius: 3%;
  background-color: #fff;
  border-bottom: 1px solid #f2f2f2;
}
.title{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  color: #666;
  box-sizing: border-box;
  padding:0 30rpx;
  border-bottom: 1px solid #f2f2f2;
  position: relative;
}
.btn{
  width: 136rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #3850e8;
  text-align: center;
  position: absolute;
  top: 22%;
  right: 30rpx;
  background-color: #f5f5f5;
  border-radius: 25% 25%;
}
.txt{
  width: 100%;
  height: 205rpx;
  position: relative;
  box-sizing: border-box;
  padding:25rpx 30rpx;
}
.txt view{
  margin: 10rpx 0;
}
.date{
  color: #666;
}
.value{
  color: #3850e8;
}