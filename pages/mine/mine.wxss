page {
  height: 100%;
  background-color: #f1f1f1;
}
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #e9e9e9;
  box-sizing: border-box;
  padding-bottom: 50rpx;
}
.mineInfo{
  width: 100%;
  height: 454rpx;
  box-sizing: border-box;
  padding: 35rpx 30rpx;
  background-color: #fff;
  /* position: relative; */
}
.mineInfo_bg{
width: 690rpx;
height: 376rpx;
position: relative;
/* top: 0;
left: 0; */
box-sizing: border-box;
padding: 50rpx 0 50rpx 30rpx;
background-size: cover;
background-image: url("data:image/png;base64,");
}
.mineInfo_bg .userHeader{
  width: 114rpx;
  height: 114rpx;
}
.mineInfo_bg .userId{
  position: absolute;
  top: 50rpx;
  left: 25%;
  font-size: 30rpx;
  color: #fff;
}
.HKT{
  width: 100%;
  color: #fff;
  font-size: 28rpx;
  position: absolute;
  bottom: 50rpx;
  left: 0rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.HKT_li view{
  text-align: center;
}
.huiyuan{
  width: 178rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin: 15rpx 0;
  box-sizing: border-box;
  padding-left:28%;
  font-size: 24rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
.userScore{
  width: 230rpx;
  height: 64rpx;
  color: #977112;
  font-size: 26rpx;
  line-height: 64rpx;
  text-align: center;
  position: absolute;
  top: 70rpx;
  right:-30rpx;
  background-size: cover;
  background-image: url("data:image/png;base64,");
}
.record{
  width: 100%;
  height: 272rpx;
  box-sizing: border-box;
  padding: 30rpx 0;
}
.record_mg{
  width: 100%;
  height: 212rpx;
  box-sizing: border-box;
  padding: 40rpx 0;
  background-color: #fff;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.record_type{
  width: 33.3%;
  height: 100%;
  font-size: 28rpx;
  text-align: center;
  border-left: 1px solid #e9e9e9;
}
image{
  width: 78rpx;
  height: 72rpx;
}
.settings{
  width: 100%;
  box-sizing: border-box;
  padding: 0rpx 25rpx;
  background-color: #fff;
}
.settings_info{
  height: 112rpx;
  line-height: 112rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #e9e9e9;
  position: relative;
}
.settings_info .img{
  width: 40rpx;
  height: 37rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.settings_info .abs{
  width: 13rpx;
  height: 23rpx;
  line-height: 112rpx;
  position: absolute;
  top: 50rpx;
  right: 0;
}