page {
  height: 100%;
  background-color: #f1f1f1;
}
.container{
  display: flex;
  flex-direction: column;
  height: 100%;
  box-sizing: border-box;
  padding: 30rpx;
}
.module{
  width: 690rpx;
  height: 175rpx;
  box-sizing: border-box;
  padding: 30rpx;
  position: relative;
  /* display: flex;
  justify-content: space-between;
  align-items: center; */
  background-size: cover;
  background-image: url("data:image/png;base64,");
}
.module_pic{
  width: 88rpx;
  height: 88rpx;
  line-height: 175rpx;
  position: absolute;
  left: 30rpx;
  top: 40rpx;
}
.module_info{
  position: absolute;
  left: 145rpx;
  font-size: 28rpx;
  color: #333;
}
.name{
  color: #666;
}
.delete_btn{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 30rpx;
  top: 55rpx;
}
.add_btn {
  width: 690rpx;
  margin: 40rpx;
  color: #fff;
  font-weight: normal;
  background-color: #3850e8;
}