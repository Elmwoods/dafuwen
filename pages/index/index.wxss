page {
  height: 100%;
}
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.banner image{
  width: 100%;
  height: 240rpx;
}
.content{
  width: 100%;
  box-sizing: border-box;
  padding: 50rpx 30rpx 50rpx 23rpx;
}
.module{
  width: 708rpx;
  height: 328rpx;
  margin-bottom: 25rpx;
  position: relative;
  background-size: cover;
  background-image: url("data:image/png;base64,");
}

.module_left{
  position: absolute;
  top: 18%;
  left: 28rpx;
  bottom: 15%;
  width: 31%;
  /* height: 80%; */
}
.module_left image{
  width: 100%;
  height: 100%;
}
.module_right{
  position: absolute;
  top: 28rpx;
  left: 40%;
  right: 28rpx;
  bottom: 28rpx;
  font-size: 24rpx;
  color: #666;
}
.right_title{
  color: #333333;
  font-size: 30rpx;
  position: relative;
  font-size: 24rpx;
}
.right_title .btn{
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 10rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.blue{
  border: 1px solid #50a6fe;
  color: #50a6fe;
}
.red{
  border: 1px solid #fe1525;
  color: #fe1525;
}
.right_line{
  height: 46rpx;
  line-height: 46rpx;
}